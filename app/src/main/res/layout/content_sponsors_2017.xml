                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:padding="5dp"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/sellad"
                    android:padding="5dp"
                    android:layout_weight="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:paddingLeft="5dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/topc"
                        android:padding="3dp"
                        android:layout_weight="1"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/amigo"
                        android:padding="3dp"
                        android:layout_weight="1"/>

                </LinearLayout>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/persi"
                    android:padding="5dp"
                    android:layout_weight="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:paddingLeft="10dp">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/yi"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:padding="5dp"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/gauri"
                    android:padding="5dp"
                    android:layout_weight="1"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Media Partner"
                android:textColor="#FFF"
                android:textSize="25dp"
                android:id="@+id/sp7"
                android:textAlignment="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:paddingLeft="10dp">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/rcn"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:padding="5dp"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dainik"
                    android:padding="5dp"
                    android:layout_weight="1"
                    android:layout_gravity="center"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="3dp"
                android:paddingBottom="3dp"
                android:paddingLeft="10dp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/hitavada"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:padding="5dp"/>
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/sakal"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:padding="5dp"/>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Networking Partner"
                android:textColor="#FFF"
                android:textSize="25dp"
                android:id="@+id/sp6"
                android:textAlignment="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/c2sms"
                android:layout_gravity="center"
                android:padding="8dp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Pizza Partner"
                android:textColor="#FFF"
                android:textSize="25dp"
                android:id="@+id/sp5"
                android:textAlignment="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/domino"
                android:layout_gravity="center"
                android:padding="8dp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>






        </LinearLayout>
    </ScrollView>



</RelativeLayout>
