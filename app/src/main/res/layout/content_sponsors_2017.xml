                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:src="@drawable/alumni"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Online Shopping Partner"
                android:textColor="#FFF"
                android:textSize="25dp"
                android:id="@+id/sp12"
                android:textAlignment="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:src="@drawable/ebay"
                android:id="@+id/ebay"
                android:onClick="sponsors"
                android:background="@drawable/active_background"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#a9a6a6"/>
