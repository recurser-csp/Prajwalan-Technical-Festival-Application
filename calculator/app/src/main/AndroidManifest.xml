<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nilesh.calculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nilesh.toast"
            android:label="@string/title_activity_toast"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".toast" />
        <activity
            android:name=".intnentdata"
            android:label="@string/title_activity_intnentdata"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".intentdatareceiver"
            android:label="@string/title_activity_intentdatareceiver"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>